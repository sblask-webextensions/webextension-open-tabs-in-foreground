{
    "author": "Sebastian Blask",
    "devDependencies": {
        "eslint": "3.13.x",
        "jscs": "2.6.x",
        "web-ext": "1.9.x"
    },
    "license": "MIT",
    "name": "opentabsinforeground",
    "repository": {
        "type": "git",
        "url": "https://github.com/sblask/webextension-open-tabs-in-foreground"
    },
    "scripts": {
        "build": "web-ext build --artifacts-dir . --ignore-files screenshots --ignore-files package.json",
        "help": "web-ext --help",
        "lint": "eslint background.js content.js && jscs background.js content.js",
        "start": "GTK_THEME=Greybird web-ext run --verbose --firefox firefox-trunk",
        "start:stable": "GTK_THEME=Greybird web-ext run --verbose --firefox firefox",
        "test": "web-ext lint"
    },
    "title": "Open Tabs In Foreground",
    "version": "1.0.0"
}
