{
    "author": "Sebastian Blask",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "all_frames": true,
            "js": [
                "content.js"
            ],
            "matches": [
                "file://*/*",
                "http://*/*",
                "https://*/*"
            ],
            "run_at": "document_start"
        }
    ],
    "description": "Open Tabs In Foreground",
    "homepage_url": "https://github.com/sblask/webextension-open-tabs-in-foreground",
    "icons": {
        "16": "icon-16x16.png",
        "32": "icon-32x32.png",
        "48": "icon-48x48.png",
        "64": "icon-64x64.png",
        "128": "icon-128x128.png"
    },
    "manifest_version": 2,
    "name": "Open Tabs In Foreground",
    "permissions": [
        "<all_urls>"
    ],
    "version": "1.0.3"
}
